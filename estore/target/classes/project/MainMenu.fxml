<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="500.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="project.controllers.MenuController">
   <children>
      <Label layoutX="14.0" layoutY="14.0" prefHeight="76.0" prefWidth="206.0" text="Menu">
         <font>
            <Font name="SansSerif Bold" size="70.0" />
         </font>
      </Label>
      <Button fx:id="btn_login" layoutX="200.0" layoutY="181.0" mnemonicParsing="false" onAction="#showLogin" prefHeight="34.0" prefWidth="200.0" style="-fx-background-color: #999999; -fx-cursor: hand;" text="Login">
         <font>
            <Font name="Arial Rounded MT Bold" size="18.0" />
         </font>
      </Button>
      <Button fx:id="btn_register" layoutX="200.0" layoutY="233.0" mnemonicParsing="false" onAction="#showRegister" prefHeight="34.0" prefWidth="200.0" style="-fx-background-color: #999999; -fx-cursor: hand;" text="Register">
         <font>
            <Font name="Arial Rounded MT Bold" size="18.0" />
         </font>
      </Button>
      <Button fx:id="btn_exit" layoutX="200.0" layoutY="286.0" mnemonicParsing="false" onAction="#exitApplication" prefHeight="34.0" prefWidth="200.0" style="-fx-background-color: #999999; -fx-cursor: hand;" text="Exit">
         <font>
            <Font name="Arial Rounded MT Bold" size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
